        "source-map-support": "^0.5.21",
        "tslib": "^2.2.0",
        "typescript": "^4.0.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/cheerio": "^0.22.31",
        "@types/cli-progress": "^3.11.0",
        "@types/express": "^4.17.11",
        "@types/node": "^14.10.2",
